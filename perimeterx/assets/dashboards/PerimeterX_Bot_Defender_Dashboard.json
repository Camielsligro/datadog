                            },
                            "groupBy": [
                                {
                                    "facet": "@http.url_details.path",
                                    "limit": 10,
                                    "sort": {
                                        "aggregation": "count",
                                        "order": "desc"
                                    }
                                }
                            ],
                            "index": "*",
                            "search": {
                                "query": "source:perimeterx $EventName $TrueIP $Continent $Country $Domain $ApplicationID"
                            }
