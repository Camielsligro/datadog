                },
                "name": "query1",
                "indexes": ["*"],
                "group_by": [
                  {
                    "facet": "@bottomline.mainframe.activity.resource.code",
                    "sort": {
                      "metric": "@bottomline.mainframe.activity.resource.duration",
                      "aggregation": "avg",
                      "order": "desc"
                    },
                    "limit": 10
                  }
                ]
              }
            ],
            "style": {
