                                        {
                                            "data_source": "metrics",
                                            "name": "query0",
                                            "query": "max:response_time_seconds_mean.quantile{$HTTP_Method,$HTTP_URL,timewindow:10000}"
                                        }
                                    ],
                                    "response_format": "timeseries",
                                    "style": {
                                        "palette": "blue",
