                                    },
                                    "display_type": "line"
                                },
                                {
                                    "formulas": [
                                        {
                                            "alias": "Min",
                                            "formula": "default_zero(query0) * 1000"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "data_source": "metrics",
