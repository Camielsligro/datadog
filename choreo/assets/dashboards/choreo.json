                                "max",
                                "value",
                                "sum"
                            ],
                            "type": "timeseries",
                            "requests": [
                                {
                                    "formulas": [
                                        {
                                            "alias": "Max",
                                            "formula": "default_zero(query1) * 1000"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "name": "query1",
                                            "data_source": "metrics",
                                            "query": "max:response_time_seconds_max.quantile{$HTTP_Method,$HTTP_URL,timewindow:10000}"
