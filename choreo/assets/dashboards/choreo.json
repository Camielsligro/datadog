        {
            "id": 2970687532529776,
            "definition": {
                "title": "Throughput And Latency",
                "background_color": "purple",
                "show_title": true,
                "type": "group",
                "layout_type": "ordered",
                "widgets": [
                    {
                        "id": 1488801576567160,
                        "definition": {
                            "title": "Throughput (requests per second) over time",
                            "title_size": "16",
                            "title_align": "left",
                            "show_legend": true,
                            "legend_layout": "horizontal",
                            "legend_columns": [
                                "avg",
                                "min",
                                "max",
                                "value",
                                "sum"
                            ],
                            "type": "timeseries",
                            "requests": [
                                {
                                    "formulas": [
                                        {
                                            "alias": "Success",
                                            "formula": "query1"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "name": "query1",
                                            "data_source": "metrics",
                                            "query": "sum:requests_total_value.count{$HTTP_Method,$HTTP_URL,http_status_code_group:2xx}.as_rate().fill(linear)"
                                        }
                                    ],
                                    "response_format": "timeseries",
                                    "style": {
                                        "palette": "green",
                                        "line_type": "solid",
                                        "line_width": "normal"
                                    },
                                    "display_type": "line"
                                },
                                {
                                    "formulas": [
                                        {
                                            "alias": "Error",
                                            "formula": "query0"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "data_source": "metrics",
                                            "name": "query0",
                                            "query": "sum:requests_total_value.count{http_status_code_group:5xx,$HTTP_Method,$HTTP_URL}.as_rate().fill(linear)"
                                        }
                                    ],
                                    "response_format": "timeseries",
                                    "style": {
                                        "palette": "red",
                                        "line_type": "solid",
                                        "line_width": "normal"
                                    },
                                    "display_type": "line"
                                }
                            ]
                        },
                        "layout": {
                            "x": 0,
                            "y": 0,
                            "width": 12,
                            "height": 3
                        }
                    },
                    {
                        "id": 3640553160784235,
                        "definition": {
                            "title": "Response Time (milliseconds) over time",
